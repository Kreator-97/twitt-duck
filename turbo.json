{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {},
    "cypress:open": {},
    "cypress:e2e": {},
    "start-test-server": {},
    "seed:test": {},
    "seed:dev": {},
    "migrate:dev": {},
    "migrate:test": {},
    "test:web": {
      "dependsOn": ["start-test-server", "twitt-duck-web#cypress:e2e", "twitt-duck-web#dev"]
    },
    "test:cypress": {
      "dependsOn": ["start-test-server", "twitt-duck-web#cypress:open", "twitt-duck-web#dev"]
    },
    "test:api": {
      "dependsOn": ["start-test-server","twitt-duck-api#test"]
    },
    "build:web": {
      "cache": false,
      "outputs": [
        "dist/**"
      ]
    },
    "build:api": {
      "cache": false,
      "outputs": [
        "dist/**"
      ],
      "dependsOn": ["build:web"]
    },
    "start": {
      "outputs": []
    }
  }
}