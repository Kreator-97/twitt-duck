{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "build:web": {
      "cache": false,
      "outputs": [
        "dist/**"
      ]
    },
    "build:api": {
      "cache": false,
      "outputs": [
        "dist/**"
      ]
    },
    "start": {
      "outputs": []
    }
  }
}